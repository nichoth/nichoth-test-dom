{
  "compilerOptions": {
    "types": ["node"],
    "target": "es2018",
    "lib": ["es2022"],
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "maxNodeModuleJsDepth": 0
  }
}
